<resources>


    <!--********************ACTION BAR STYLES*********************-->

    <style name="NR_ActionBar" parent="android:Widget.ActionBar">
        <item name="android:background">@color/blackRed</item>
        <item name="android:iconifiedByDefault">false</item>
        <item name="android:icon">@color/lightBlue</item>
        <item name="android:actionBarSize">wrap_content</item>
        <item name="android:titleTextStyle">@style/NR_ActionBar_Title_Text</item>
    </style>
    
    <style name="NR_ActionBar_Title_Text" parent="android:TextAppearance">
        <item name="android:textColor">@color/grey</item>
        <item name="android:title">@string/action_bar_app_name</item>
        <item name="android:textSize">16sp</item>
        <item name="android:typeface">monospace</item>
    </style>

    <!--********************DIALOG STYLES********************-->

    <style name="NR_Dialog" parent="android:Theme.Dialog">
        <item name="android:textAppearance">@color/lightBlue</item>
        <item name="android:typeface">monospace</item>
        <item name="android:titleTextStyle">@style/NR_AlertDialogTitle</item>
        <item name="android:background">@color/darkBlackRed</item>
    </style>

    <style name="NR_AlertDialogTitle">
        <item name="android:textColor">@color/red</item>
        <item name="android:typeface">monospace</item>
    </style>

    <style name="NR_SettingsActivity">
        <item name="android:dialogTheme">@style/NR_Dialog</item>
        <item name="android:background">@color/darkBlackRed</item>
        <item name="android:textColor">@color/red</item>
    </style>



